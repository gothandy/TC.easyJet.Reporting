<h2>Toggl</h2>

<table>
    <thead>
        <tr>
            <th>Month</th>
            <th>UserName</th>
            <th>DomId</th>
            <th>HouseKeeping</th>
            <th>Billable</th>
        </tr>
    </thead>
    <tbody>
    @foreach (Vincente.Azure.Entities.TimeEntryEntity item in Model)
    {
        <tr>
            <td>@string.Format("{0:d}", item.Month)</td>
            <td>@item.UserName</td>
            <td>@item.DomId</td>
            <td>@item.Housekeeping</td>
            <td class="currency">@string.Format("{0:n2}", ((decimal)item.Billable) / 100)</td>
        </tr>
    }
    </tbody>
</table>

