@model IEnumerable<Vincente.Data.Entities.TimeEntry>
<h2>Data</h2>
<h3>Toggl</h3>
<table>
    <thead>
        <tr>
            <th>Month</th>
            <th>UserName</th>
            <th>DomId</th>
            <th>HouseKeeping</th>
            <th>Billable</th>
        </tr>
    </thead>
    <tbody>
    @foreach (Vincente.Data.Entities.TimeEntry item in Model)
    {
        <tr>
            <td>@string.Format("{0:d}", item.Month)</td>
            <td>@item.UserName</td>
            <td>@item.DomId</td>
            <td>@item.Housekeeping</td>
            <td class="currency">@string.Format("{0:n2}", item.Billable)</td>
        </tr>
    }
    </tbody>
</table>

