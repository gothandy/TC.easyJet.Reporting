@model List<NavLink>

@if (Model.Count != 0)
{
    <div class="container">
        <ol class="breadcrumb">
            @foreach (NavLink link in Model)
            {
                <li>@Html.ActionLink(link.LinkText, link.ActionName, link.ControllerName, link.RouteValues, null)</li>

            }
            <li class="visible-xs-inline">@ViewBag.Title</li>
        </ol>
    </div>
}
