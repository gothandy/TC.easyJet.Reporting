@model IEnumerable<CardWithTime>
<h2>Data</h2>
<h3>All By Month</h3>
<table>
    <thead>
        <tr>
            <th>Month</th>
            <th>Epic</th>
            <th>TeamName</th>
            <th>Reuse DA</th>
            <th>Reuse FCP</th>
            <th>Billable</th>
        </tr>
    </thead>
    <tbody>
        @foreach (CardWithTime item in Model)
        {
            <tr>
                <td>@DateTimeHelper.Format(item.Month)</td>
                <td>@item.Epic</td>
                <td>@item.TeamName</td>
                <td>@item.ReuseDA</td>
                <td>@item.ReuseFCP</td>
                <td class="currency">@CurrencyHelper.Format(item.Billable)</td>
            </tr>
        }
    </tbody>
</table>
