@model IEnumerable<Card>
<h2>Data</h2>
<h3>Trello</h3>
<table>
    <thead>
        <tr>
            <th>Epic</th>
            <th>Index</th>
            <th>List</th>
            <th>DomId</th>
            <th>Name</th>
            <th>Invoice</th>
        </tr>
    </thead>
    <tbody>
    @foreach (Card card in Model)
    {
        <tr>
            <td>@card.Epic</td>
            <td class="number">@card.ListIndex</td>
            <td>@card.ListName</td>
            <td>@card.DomId</td>
            <td>@TrelloHelper.CardLink(card.Id, card.Name)</td>
            <td>@DateTimeHelper.Format(card.Invoice)</td>
        </tr>
    }
    </tbody>
</table>
