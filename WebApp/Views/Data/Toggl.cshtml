@model IEnumerable<TimeEntryEntity>
<h2>Data</h2>
<h3>Toggl</h3>
<table>
    <thead>
        <tr>
            <th>Start</th>
            <th>UserName</th>
            <th>DomId</th>
            <th>HouseKeeping</th>
            <th>Billable</th>
        </tr>
    </thead>
    <tbody>
    @foreach (TimeEntryEntity item in Model)
    {
        <tr>
            <td>@string.Format("{0:d}", item.Start)</td>
            <td>@item.UserName</td>
            <td>@item.DomId</td>
            <td>@item.Housekeeping</td>
            <td class="currency">@string.Format("{0:n2}", ((decimal)item.Billable) / 100)</td>
        </tr>
    }
    </tbody>
</table>

